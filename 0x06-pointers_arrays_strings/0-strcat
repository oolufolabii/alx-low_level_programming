#include "main.h"

/**
 * _strcat - concatenates two strings(dest and src)
 * @dest: string to append to
 * @src: string to add
 * Return: a pointer to the resulting string dest
 */

char *_strcat(char *dest, char *src)

{
int a, b;

a = 0;
b = 0;

while (dest[a] != '\0')
a++; //end of dest string

while (src[b] != '\0')
{
dest[a] = src[b]; //cantenation
b++;
a++;
}

dest[a] = '\0';

return (dest);

}
